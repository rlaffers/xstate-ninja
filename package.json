{
  "name": "xstate-insights",
  "version": "0.0.0-development",
  "description": "Devtool for XState state machines",
  "source": "src/index.js",
  "module": "dist/index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "lint": "eslint src/",
    "format": "prettier --write README.md src/**/*.js",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "build": "parcel build",
    "watch": "parcel watch",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "xstate"
  ],
  "author": "Richard Laffers",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.2"
  },
  "peerDependencies": {
    "xstate": "^4.30.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/rlaffers/xstate-insights",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlaffers/xstate-async-guards"
  },
  "volta": {
    "node": "16.15.0",
    "npm": "8.11.0"
  }
}
