{
  "name": "magic-json-tree",
  "version": "1.1.1",
  "author": "Richard Laffers",
  "license": "ISC",
  "keywords": [
    "svelte",
    "json"
  ],
  "type": "module",
  "main": "./dist/magic-json-tree.umd.cjs",
  "module": "./dist/magic-json-tree.js",
  "svelte": "lib/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/*.cjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/magic-json-tree.js",
      "require": "./dist/magic-json-tree.umd.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint lib/ src/",
    "lint:fix": "eslint --fix lib/ src/",
    "prepublish": "pnpm check"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tsconfig/svelte": "^5.0.2",
    "eslint": "8",
    "svelte": "^3.59.2",
    "svelte-check": "^3.5.1",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.4.0",
    "typescript": "^5.2.2",
    "vite": "^3.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rlaffers/xstate-ninja"
  },
  "volta": {
    "node": "18.17.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
