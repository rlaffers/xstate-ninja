{
  "name": "@xstate-ninja/vue",
  "version": "1.0.1",
  "description": "XState Ninja integration for Vue",
  "type": "module",
  "main": "./dist/xstate-ninja-vue.umd.cjs",
  "module": "./dist/xstate-ninja-vue.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/*.cjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/xstate-ninja-vue.js",
      "require": "./dist/xstate-ninja-vue.umd.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "concurrently \"tsc --watch --noEmit false --emitDeclarationOnly true\" \"vite build --watch\"",
    "build": "vite build && tsc --noEmit false --emitDeclarationOnly true",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "check": "tsc --noEmit"
  },
  "devDependencies": {
    "@xstate/vue": "^2.0.0",
    "concurrently": "^7.3.0",
    "eslint": "8",
    "tslib": "^2.4.0",
    "typescript": "^5.2.2",
    "vite": "^3.0.0",
    "vue": "^3.3.4",
    "xstate": "^4.38.2",
    "xstate-ninja": "workspace:^1.2.0"
  },
  "peerDependencies": {
    "@xstate/vue": "^2.0.0",
    "vue": "^3.3.4",
    "xstate": "^4.33.6",
    "xstate-ninja": "^1.0.0"
  },
  "keywords": [
    "xstate",
    "fsm",
    "state machine",
    "vue"
  ],
  "license": "MIT",
  "author": {
    "name": "Zehir",
    "email": "zehir@zorim.fr",
    "url": "https://github.com/Zehir"
  },
  "maintainers": [
    {
      "name": "Zehir",
      "email": "zehir@zorim.fr",
      "url": "https://github.com/Zehir"
    }
  ],
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/Zehir"
  },
  "homepage": "https://github.com/rlaffers/xstate-ninja",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlaffers/xstate-ninja"
  },
  "volta": {
    "node": "18.17.0"
  }
}
