{
  "name": "@xstate-ninja/react",
  "version": "1.1.3",
  "description": "XState Ninja integration for React",
  "type": "module",
  "main": "./dist/xstate-ninja-react.umd.cjs",
  "module": "./dist/xstate-ninja-react.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/*.cjs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/xstate-ninja-react.js",
      "require": "./dist/xstate-ninja-react.umd.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "concurrently \"tsc --watch --noEmit false --emitDeclarationOnly true\" \"vite build --watch\"",
    "build": "vite build && tsc --noEmit false --emitDeclarationOnly true",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@xstate/react": "^3.0.1",
    "concurrently": "^7.3.0",
    "eslint": "8",
    "react": "^18.2.0",
    "tslib": "^2.4.0",
    "typescript": "^5.2.2",
    "vite": "^3.2.7",
    "xstate": "^4.38.2",
    "xstate-ninja": "workspace:^1.2.0"
  },
  "peerDependencies": {
    "@xstate/react": "^3.0.1",
    "react": "^18.2.0",
    "xstate": "^4.33.6",
    "xstate-ninja": "^1.0.0"
  },
  "keywords": [
    "xstate",
    "fsm",
    "state machine"
  ],
  "author": "Richard Laffers",
  "license": "ISC",
  "homepage": "https://github.com/rlaffers/xstate-ninja",
  "repository": {
    "type": "git",
    "url": "https://github.com/rlaffers/xstate-ninja"
  },
  "volta": {
    "node": "18.17.0"
  }
}
